/**
*_strcat - concatenates two strings
*
* @dest: first string
* @src: second string
* Return: dest
*/

char *_strcat(char *dest, char *src)
{
int i;
int j = strlen(src);

for (i = 0; src[i]; i++)
{
dest[i + j] = src[i];
}
dest[i + j] = '\0';
