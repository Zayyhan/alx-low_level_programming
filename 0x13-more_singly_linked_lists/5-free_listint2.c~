#include "lists.h"

/**
 * free_listint2 - frees a listint_t list, sets the head to NULL
 *
 * @head: double pointer to head of list
 * Return: void
 */

void free_listint2(listint_t **head)
{
	listint_t *tmp;

	if (head)
	{
		while (*head)
		{
			tmp = *head;
			*head = temp->next;
			free(tmp);
		}
	}
}
